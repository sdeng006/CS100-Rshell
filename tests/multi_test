echo multi tests
echo Single AND:
ls -l && echo hello world
echo Double AND: 000000 && echo AAA111 && echo BBB222
echo Single OR:
something not working || echo Second something should work
echo Double OR: || echo should not work || echo should work
echo Single Semicolon:; echo This should work
echo Double Semicolon:; echo first one; echo second one 
echo
echo three Semicolon:
echo 1; echo 2; echo 3
echo two AND:
ls && echo 1 + 1 && print = 2
echo two OR:
ls || should not work || should work 1
echo testing exit
exit